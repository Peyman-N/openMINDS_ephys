{
    "_type": "https://openminds.ebrains.eu/ephys/CulturingActivity",
    "_extends": "/core/research/protocolExecution.schema.tpl.json",
    "required": [
        "cultureType",
        "cultureSolution"
    ],
    "properties": {
        "cultureType": {
            "_instruction": "Type of culture (e.g. primary, secondary)",
            "_linkedTypes": [
                "https://openminds.ebrains.eu/controlledTerms/CultureType"
            ]
        },
        "cultureAge": {
            "_instruction": "Age of culture at time of data collection",
            "_embeddedTypes": [
                "https://openminds.ebrains.eu/core/QuantitativeValue",
                "https://openminds.ebrains.eu/core/QuantitativeValueRange"
            ]
        },
        "cultureSolution": {
            "type": "string",
            "_instruction": "Solution the culture was maintained in prior to the experiment"
        },
        "input": {
            "_linkedTypes": [
                "https://openminds.ebrains.eu/core/SubjectGroupState",
                "https://openminds.ebrains.eu/core/SubjectState"
            ]
        },
        "output": {
            "_linkedTypes": [
                "https://openminds.ebrains.eu/core/TissueSampleState"
            ]
        }
    }
}